    "import re\n",
    "from decimal import Decimal\n",
    "\n",
    "def parse_price_number(price_text: str):\n",
    "    if not price_text:\n",
    "        return None\n",
    "    # Keep digits and dot only (works for \"$199.99\")\n",
    "    m = re.search(r'(\\d+(?:\\.\\d{1,2})?)', price_text.replace(',', ''))\n",
    "    return float(m.group(1)) if m else None\n",
    "\n",
    "def scrape_amazon_redmi(pages=3):\n",
    "    products = []\n",
    "\n",
    "    for page in range(1, pages + 1):\n",
    "        url = BASE_URL.format(page)\n",
    "        response = requests.get(url, headers=HEADERS, timeout=20)\n",
    "        soup = BeautifulSoup(response.text, \"lxml\")\n",
    "\n",
    "        items = soup.select('div[data-component-type=\"s-search-result\"]')\n",
    "\n",
    "        for item in items:\n",
    "            name = item.select_one(\"h2 a span\")\n",
    "            name = name.get_text(strip=True) if name else None\n",
    "\n",
    "            # Price: $199.99 is typically in a-offscreen\n",
    "            price_text = item.select_one(\"span.a-price > span.a-offscreen\")\n",
    "            price_text = price_text.get_text(strip=True) if price_text else None\n",
    "            price = parse_price_number(price_text)\n",
    "\n",
    "            rating = item.select_one(\"span.a-icon-alt\")\n",
    "            rating = rating.get_text(strip=True) if rating else None\n",
    "\n",
    "            reviews = item.select_one(\"span.a-size-base.s-underline-text\")\n",
    "            reviews = reviews.get_text(strip=True) if reviews else None\n",
    "\n",
    "            link = item.select_one(\"h2 a\")\n",
    "            link = (\"https://www.amazon.in\" + link[\"href\"]) if link and link.get(\"href\") else None\n",
    "\n",
    "            products.append({\n",
    "                \"name\": name,\n",
    "                \"price\": price,          # numeric (float) like 199.99\n",
    "                \"price_text\": price_text,# optional: keep original \"$199.99\"\n",
    "                \"rating\": rating,\n",
    "                \"reviews\": reviews,\n",
    "                \"url\": link\n",
    "            })\n",
    "\n",
    "        time.sleep(random.uniform(2, 4))\n",
    "\n",
    "    return products\n"
   ]
